{
  "compilerOptions": {
    "target": "ES2021",
    "module": "commonjs",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": ".",
    "strict": true,
    "skipLibCheck": true,
    "outDir": "./dist",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@env": [
        "./env.ts"
      ]
    }
  },
}